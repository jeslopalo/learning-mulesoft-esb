<?xml version="1.0" encoding="UTF-8"?>

<namespace xmlns="http://www.mulesoft.org/schema/mule/tooling.attributes" xmlns:mule="http://www.mulesoft.org/schema/mule/core" url="http://www.mulesoft.org/schema/mule/hello-world" prefix="hello-world">
  <global-cloud-connector versions="[3.5.0,8.0.0]" caption="HelloWorld" description="" icon="" localId="parentConfig" image="" abstract="true" extends="http://www.mulesoft.org/schema/mule/hello-world/abstractHello-worldConnectorGeneric"></global-cloud-connector>
  <global-cloud-connector versions="[3.5.0,8.0.0]" caption="HelloWorld: Configuration type strategy" description="Global Configuration type strategy configuration information. Configuration type Strategy." icon="icons/small/hello-world-connector-24x16.png" localId="config-type" image="icons/large/hello-world-connector-48x32.png" abstract="false" extends="http://www.mulesoft.org/schema/mule/hello-world/parentConfig" connectivityTesting="off" metaData="static" metaDataKeyDisplay="DISPLAY_NAME">
    <attribute-category caption="General" description="Hello-world configuration properties.">
      <group id="hello-worldGenericProperties" caption="Basic Settings">
        <name name="name" caption="Name" description="Give a name to this configuration so it can be later referenced by config-ref." required="true"></name>
      </group>
      <group id="general" caption="General">
        <string defaultValue="Hello" name="greeting" caption="Greeting" description="Configurable." required="false" javaType="java.lang.String"></string>
        <string defaultValue="How are you?" name="reply" caption="Reply" description="Configurable." required="false" javaType="java.lang.String"></string>
      </group>
    </attribute-category>
  </global-cloud-connector>
  <cloud-connector caption="Abstracthello-worldconnectorgeneric" description="Interact with Hello-world." icon="icons/small/hello-world-connector-24x16.png" localId="abstractHello-worldConnectorGeneric" image="icons/large/hello-world-connector-48x32.png" abstract="true">
    <attribute-category caption="General" description="General.">
      <group id="abstractHello-worldConnectorGeneric" caption="Basic Settings">
        <globalRef requiredType="http://www.mulesoft.org/schema/mule/hello-world/config-type" name="config-ref" caption="Connector Configuration" description="Specify which configuration to use for this invocation." required="true"></globalRef>
      </group>
    </attribute-category>
  </cloud-connector>
  <cloud-connector versions="[3.5.0,8.0.0]" aliasId="org.mule.tooling.ui.modules.core.pattern.hello-world" caption="HelloWorld" description="Anypoint Connector." icon="icons/small/hello-world-connector-24x16.png" localId="hello-world-connector" image="icons/large/hello-world-connector-48x32.png" extends="http://www.mulesoft.org/schema/mule/hello-world/abstractHello-worldConnectorGeneric">
    <attribute-category caption="General" description="General.">
      <group id="hello-worldConnectorGeneric" caption="Basic Settings">
        <modeSwitch alwaysCombo="true" name="Hello-world operations to execute" caption="Operation" description="Operation.">
          <no-operation abstractElement="http://www.mulesoft.org/schema/mule/hello-world/abstractHello-worldConnectorGeneric" versions="[3.5.0,8.0.0]" connector-name="processor"></no-operation>
          <mode modeId="http://www.mulesoft.org/schema/mule/hello-world/greet" modeLabel="Greet"></mode>
        </modeSwitch>
      </group>
    </attribute-category>
  </cloud-connector>
  <cloud-connector returnType="java.lang.String" caption="Greet" icon="icons/small/hello-world-connector-24x16.png" localId="greet" image="icons/large/hello-world-connector-48x32.png" abstract="true" extends="http://www.mulesoft.org/schema/mule/hello-world/abstractHello-worldConnectorGeneric" metaData="static">
    <attribute-category caption="General" description="General.">
      <group id="general" caption="General">
        <string name="friend" caption="Friend" description="Content to be processed." required="true" javaType="java.lang.String"></string>
      </group>
    </attribute-category>
  </cloud-connector>
